VALTYP  = $0DTYPNUM  = $00TYPSTR  = $FFFAC     = $61FACSGN  = $66CHRGET  = $73CHRGOT  = $79TEMP    = $FEIEVAL    = $030AEVALCONT = $CE8DINT2FP   = $D391FPADDAY  = $D867.BYTE <ENABLE, >ENABLE.ORG +828ENABLE:    LDA #<EVALHEX    STA IEVAL    LDA #>EVALHEX    STA IEVAL+1    RTSEVALHEX:    LDA #TYPNUM    STA VALTYP    JSR CHRGET    CMP #'$'    BEQ ISHEX    JSR CHRGOT    JMP EVALCONTISHEX:    LDX #$02    LDY #3    LDA (CHRGOT+1),Y    CMP #'0'    BCC ONEBYTE    CMP #':'    BCC HEXLOOP    CMP #'a'    BCC ONEBYTE    CMP #'g'    BCC HEXLOOPONEBYTE:    LDA #$00    PHA    DEXHEXLOOP:    JSR CHRGET    CMP #'@'    BCC SKIP1    ADC #$08SKIP1:    ASL    ASL    ASL    ASL    STA TEMP    JSR CHRGET    CMP #'@'    BCC SKIP2    ADC #$08SKIP2:    AND #$0F    ORA TEMP    PHA    DEX    BNE HEXLOOP    PLA    TAY    PLA    JSR INT2FP; INT2FP DOES A SIGNED CONVERSION; - CHECK FOR NEGATIVE AND CORRECT    LDA FACSGN    AND #$80    BEQ POSITIVE    LDA #<C65536    LDY #>C65536    JSR FPADDAYPOSITIVE:    JMP CHRGET; 65536.0 AS FP VALUEC65536: .BYTE +145,0,0,0,0,+255 